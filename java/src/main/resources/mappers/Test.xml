<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ihr_mini.Mapper.TestMapper">
    <select id="getInfor" resultType="com.example.ihr_mini.Entity.Test">
        select * from test
    </select>
    <delete id="getone" parameterType="String">
        select from test where id =#{id}
    </delete>
    <insert id="addone" parameterType="com.example.ihr_mini.Entity.Test">
        insert into test(id,name)values(#{id},#{name})
    </insert>
</mapper>